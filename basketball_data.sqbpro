</sql><sql name="DeDuplication*">WITH RankedGames AS (
    SELECT &quot;GAME-ID&quot;, &quot;TEAM&quot;, ROWID,
           ROW_NUMBER() OVER (
               PARTITION BY &quot;GAME-ID&quot;, &quot;TEAM&quot;
               ORDER BY 
                   (CASE WHEN &quot;1H&quot; IS NOT NULL AND &quot;2H&quot; IS NOT NULL THEN 1 ELSE 0 END) DESC,
                   ROWID ASC
           ) AS rn
    FROM basketball_data
)
DELETE FROM basketball_data
WHERE ROWID IN (
    SELECT ROWID FROM RankedGames WHERE rn &gt; 1
);
</sql><sql name="SQL 9*">WITH RankedTeams AS (
    SELECT &quot;GAME-ID&quot;, &quot;TEAM&quot;, ROWID,
           ROW_NUMBER() OVER (
               PARTITION BY &quot;GAME-ID&quot;
               ORDER BY ROWID ASC
           ) AS rn
    FROM basketball_data
)
DELETE FROM basketball_data
WHERE &quot;GAME-ID&quot; IN (
    SELECT &quot;GAME-ID&quot; FROM RankedTeams WHERE rn &gt; 2
);</sql><current_tab id="7"/></tab_sql></sqlb_project>
